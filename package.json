{
  "name": "@moneko/common",
  "description": "common",
  "version": "1.7.6",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./esm/index.js"
    },
    "./*": [
      "./*"
    ]
  },
  "scripts": {
    "prebuild": "rm -rf ./lib && rm -rf ./esm && tsc",
    "build": "swc src -d lib --strip-leading-paths -C exclude=__tests__ --copy-files",
    "postbuild": "swc src -d esm --strip-leading-paths -C module.type=es6 -C jsc.target=esnext -C exclude=__tests__ --copy-files",
    "test": "jest --watchAll --coverage"
  },
  "keywords": [],
  "author": "moneko",
  "license": "MIT",
  "files": [
    "lib",
    "esm",
    "types"
  ],
  "devDependencies": {
    "@swc/cli": "0.5.0",
    "@swc/core": "1.9.2",
    "@swc/jest": "0.2.37",
    "@types/jest": "29.5.14",
    "eslint-config-neko": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
